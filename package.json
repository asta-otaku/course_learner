  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:monitor": "node scripts/dev-monitor.js",
    "dev:monitor-advanced": "node scripts/dev-monitor-advanced.js",
    "build": "next build",
    "build:force": "next build || exit 0",
    "build:vercel": "SKIP_ENV_VALIDATION=1 next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
